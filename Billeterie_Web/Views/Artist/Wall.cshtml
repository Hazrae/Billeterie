@model IEnumerable<Billeterie_Web.ViewModel.ArtistWallViewModel>

@{
    ViewData["Title"] = "Wall";
}

    <div class="row">
        @foreach (var item in Model)
        {
            <dix class="col-2">
                <div class="row">
                    <a href=@Url.Action("Profil","Artist",new {id = item.ArtistID})>
                        <img src=@item.ArtistPhoto style="max-height:100px; max-width:100px">
                    </a>
                </div>
                <div class="row" style="text-align:center">@item.ArtistName</div>
            </dix>          
        }
    </div>

